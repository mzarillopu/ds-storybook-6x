    "storybook": "npm run stencil-build && start-storybook -s ./dist -p 6006",
    "build": "webpack --config ./webpack.prod.config.js --mode production",
    "build-dev": "webpack --config ./webpack.dev.config.js --mode development",
    "build-css-dev": "webpack --config ./webpack.css.dev.config.js --mode development",
